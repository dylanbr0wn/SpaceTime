{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "globalDependencies": [
      ".eslintrc",
      "tsconfig.json",
      ".prettierrc",
      ".eslintignore"
    ],
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "build/**"
        ]
      },
      "lint": {
        "outputs": []
      },

      "dev": {
        "cache": false
      }
    }
  }